{
  "$schema": "https://microsoft.github.io/chartifact/schema/idoc_v1.json",
  "title": "Feature: Chart",
  "dataLoaders": [
    {
      "dataSourceName": "chartData",
      "type": "inline",
      "format": "csv",
      "content": [
        "category,value",
        "A,20",
        "B,34",
        "C,55",
        "D,40",
        "E,67"
      ]
    }
  ],
  "groups": [
    {
      "groupId": "main",
      "elements": [
        "## Chart\nUse charts for data visualizations with Vega-Lite specifications.",
        {
          "type": "chart",
          "chartKey": "1"
        }
      ]
    }
  ],
  "resources": {
    "charts": {
      "1": {
        "$schema": "https://vega.github.io/schema/vega-lite/v6.json",
        "data": {
          "name": "chartData"
        },
        "mark": "bar",
        "encoding": {
          "x": {
            "field": "category",
            "type": "nominal",
            "title": "Category"
          },
          "y": {
            "field": "value",
            "type": "quantitative",
            "title": "Value"
          },
          "color": {
            "field": "category",
            "type": "nominal"
          }
        }
      }
    }
  }
}